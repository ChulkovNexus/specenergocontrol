<?xml version="1.0" encoding="utf-8"?>
<selector android:enterFadeDuration="200" android:exitFadeDuration="100" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/app_yellow_secondary_color"/>
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item >
        <shape >
            <solid android:color="@color/app_yellow_color"/>
            <corners android:radius="4dp" />
        </shape>
    </item>
</selector>